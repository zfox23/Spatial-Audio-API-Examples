{
  "name": "spatial-standup",
  "productName": "HiFi Spatial Standup",
  "version": "0.3.0",
  "description": "A complex demo application of High Fidelity's Spatial Audio API which integrates many of the API's features as well as Twilio Video Conferencing.",
  "main": "dist/electron.js",
  "scripts": {
    "client": "webpack --env dev",
    "client:dev": "webpack --env dev",
    "client:prod": "webpack --env prod",
    "server:build": "tsc --p ./src/server && cp -r ./src/server/static ./src/server/dist",
    "server": "npm run server:build && node -r esm ./src/server/dist/index.js dev false",
    "server:dev": "npm run server:build && node -r esm ./src/server/dist/index.js dev false",
    "server:dev:https": "npm run server:build && node -r esm ./src/server/dist/index.js dev true",
    "server:prod": "npm run client:prod && npm run server:build && node -r esm ./src/server/dist/index.js prod false",
    "audiobots": "node -r esm ./src/server/audioBots.js",
    "discord": "tsc --p ./src/discordBot/ && node ./src/discordBot/dist/index.js",
    "electron": "webpack --env electron && node -r esm ./src/electron/generateElectronApp.js && electron .",
    "make": "webpack --env electron && node -r esm ./src/electron/generateElectronApp.js && electron-forge make",
    "start": "electron-forge start",
    "package": "webpack --env electron && node -r esm ./src/electron/generateElectronApp.js && electron-forge package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/highfidelity/Spatial-Audio-API-Examples.git"
  },
  "author": "High Fidelity Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/highfidelity/Spatial-Audio-API-Examples/issues"
  },
  "homepage": "https://github.com/highfidelity/Spatial-Audio-API-Examples#readme",
  "dependencies": {
    "audio-decode": "^1.4.0",
    "audio-format": "^2.3.2",
    "aws-sdk": "^2.931.0",
    "body-parser": "^1.19.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "howler": "^2.2.1",
    "jose": "^3.11.1",
    "node-fetch": "^2.6.1",
    "pcm-convert": "^1.6.5",
    "pg": "^8.6.0",
    "process": "^0.11.10",
    "simple-color-picker": "^1.0.5",
    "twilio-video": "^2.13.1",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@types/howler": "^2.2.2",
    "@types/html-webpack-plugin": "^3.2.5",
    "@types/node-fetch": "^2.5.10",
    "@types/socket.io-client": "^1.4.36",
    "chokidar": "^3.5.1",
    "css-loader": "^5.2.0",
    "css-modules-require-hook": "^4.2.3",
    "ejs": "^3.1.6",
    "electron": "^12.0.5",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "hifi-spatial-audio": "^1.3.1-2",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.4.0",
    "nodemon": "^2.0.7",
    "postcss-loader": "^5.2.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "socket.io": "^4.0.0",
    "socket.io-client": "^4.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.1.0",
    "twilio": "^3.59.0",
    "typescript": "^4.2.3",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "src/server/static/favicon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "spatial_standup"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
